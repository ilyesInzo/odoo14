            <field name="time_ids" eval="[(0, 0, {'day': 'monday', 'work_Time': 8}),
               (0, 0, {'day': 'tuesday', 'work_Time': 8}),
               (0, 0, {'day': 'wednesday', 'work_Time': 8}),
               (0, 0, {'day': 'thursday', 'work_Time': 8}),
               (0, 0, {'day': 'friday', 'work_Time': 8}),
               (0, 0, {'day': 'saturday', 'work_Time': 0}),
               (0, 0, {'day': 'sunday', 'work_Time': 0})]" />
        </record>
    </data>
</odoo>